/*日历*/
.uni-page-head-bd>.uni-page-head__title{
	font-weight: normal;
}
.daySignin_top{
	width:100%;
	height:200rpx;
	background-color:#080;
	padding-top:50rpx;
	box-sizing: border-box;
}
.qiandaoborder{
	width:200rpx;
	height:70rpx;
	line-height:70rpx;
	border-radius:50rpx;
	border:1rpx solid #fff;
	background-color:#080;
	color:#fff;
