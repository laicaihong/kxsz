.mycollect{
	padding:30rpx 25rpx 0;
}
.search{
	width:100%;
	height:70rpx;
	background-color:#fff;
	border-radius:5rpx;
	position:relative;
	padding-left:30rpx;
	margin-bottom:15rpx;
	box-sizing: border-box;
}
page{
	background-color: #f5f5f5;
}
.searchicon{
	width:25rpx;
	height:25rpx;
	background-size:100% 100%;
	position:relative;
	top:12rpx;
}
.searchinput{
	width:100%;
	height:78rpx;
	line-height:78rpx;
	font-size:30rpx;
	position:absolute;
	top:0;
	padding-left:40rpx;
}
.inputPlace {
  color: #b2afaf;
}
.navtab{
	width:100%;
	height:125rpx;
	line-height:125rpx;
	background-color:#fff;
	border-radius:5rpx;
	position:relative;
	font-size:34rpx;
	color:#9D9D9D;
	margin-bottom:15rpx;
}
.navtab_item{
	float:left;
	width:33%;
	text-align:center;
}
.nav-active{
	color:#008F00;
}
.navtabimg{
	width:43rpx;
	height:37rpx;
	background-size:100% 100%;
	margin-right:20rpx;
	position:relative;
	top:5rpx;
}
.tip{
	text-align: center;
}

