.loginText{
	margin: 140rpx 0 0 84rpx;
	font-size: 54rpx;
	color: #3E3D3D;
}
.register{
	margin: 10rpx 0 0 84rpx;
	font-size: 26rpx;
	color: #A1A4A5;
}
.pla{
	color: #C6C9CA;
	font-size: 30rpx;
}
.loginInput{
	width: 620rpx;
	height: 90rpx;
	text-indent: 54rpx;
	color: #3E3D3D;
	font-size: 30rpx;
}
.phoneInput{
	margin: auto;
	margin-top: 60rpx;
}
/*账号登录*/
.passwordInput{
	margin: auto;
	margin-top: 40rpx;
}
/*账号登录结束*/
.loginBtn{
	width: 620rpx;
	height: 84rpx;
	margin: auto;
	margin-top: 84rpx;
	border-radius: 84rpx;
	font-size: 32rpx;
	line-height: 84rpx;
	text-align: center;
}
.loginnBtNo{
	background: #f5;
	color: #999;
}
.loginnBtnYes{
	background: #2F881E;
	color: #fff;
}
.orderLogin{
	width: 620rpx;
	height: 84rpx;
	margin: auto;
	margin-top: 34rpx;
	text-align: center;
}
.orderLogin text{
	font-size: 24rpx;
	color: #3E3D3D;
}
/*验证码登录*/
.yzmLogin{
	width: 620rpx;
	height: 90rpx;
	margin: auto;
	margin-top: 40rpx;
}
.yzmInput{
	width: 400rpx;
	height: 90rpx;
	text-indent: 54rpx;
	color: #3E3D3D;
	font-size: 30rpx;
	float: left;
}
.yzmBtn{
	font-size: 26rpx;
	color: #2F881E;
	float: right;
	line-height: 84rpx;
}
.yzmTime{
	font-size: 26rpx;
	color: #C6C9CA;
	float: right;
	line-height: 84rpx;
}
/*验证码登录结束*/
/*第三方登录*/
.orderLoginView{
	margin: auto;
	margin-top: 344rpx;
	width: 580rpx;
	height: 2rpx;
	background: #F6F6F6;
	position: relative;
}
.orderLoginView text{
	width: 208rpx;
	background: #fff;
	color: #CCCCCC;
	font-size: 28rpx;
	position: absolute;
	left: 0;
	right: 0;
	top: -20rpx;
	margin: auto;
	text-align: center;
}
.orderLoginBtn{
	margin: 44rpx 234rpx 0 234rpx;
}
.qqlogin{
	width: 54rpx;
	height: 54rpx;
	float: left;
}
.weixinlogin{
	width: 54rpx;
	height: 54rpx;
	float: right;
}
/*第三方登录结束*/

