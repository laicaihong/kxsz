<view><image class="bg-img" src="../../static/reg/bj.png" mode="widthFix"></image><view class="bottom-box"><view style="text-align:center;"><image class="logo-img" src="../../static/reg/log.png" mode="widthFix"></image><view class="title">新用户注册</view><form action><view class="form"><view class="form-item"><text class="itemtitle">姓名</text><input class="input picker" maxlength="7" type="text" value="" placeholder="请填写姓名"/></view><view class="form-item"><picker mode="selector" range="{{gender}}" data-event-opts="{{[['change',[['bindSchoolChange',['$event']]]]]}}" bindchange="__e"><text class="itemtitle">学校</text><view class="picker">{{''+gender[uploadInfo.school]+''}}<image class="xaiicon" src="../../static/my/righticon.png" mode="widthFix"></image></view></picker></view><view class="form-item"><picker mode="selector" range="{{genClass}}" data-event-opts="{{[['change',[['bindClassChange',['$event']]]]]}}" bindchange="__e"><text class="itemtitle">年级</text><view class="picker">{{''+genClass[uploadInfo.class]+''}}<image class="xaiicon" src="../../static/my/righticon.png" mode="widthFix"></image></view></picker></view><view class="form-item"><picker mode="selector" range="{{grade}}" data-event-opts="{{[['change',[['bindGradeChange',['$event']]]]]}}" bindchange="__e"><text class="itemtitle">班级</text><view class="picker">{{''+grade[uploadInfo.classClass]+''}}<image class="xaiicon" src="../../static/my/righticon.png" mode="widthFix"></image></view></picker></view><view class="form-item"><view data-event-opts="{{[['tap',[['toggleTab',['$event']]]]]}}" class="pickerpicker" bindtap="__e"><text class="itemtitle">家庭住址</text><view class="picker"><block wx:if="{{uploadInfo.region.length==0}}"><text></text></block><block wx:else><text class="pickerText">{{''+uploadInfo.region[0]+"，"+uploadInfo.region[1]+"，"+uploadInfo.region[2]+''}}</text></block><image class="xaiicon" src="../../static/my/righticon.png" mode="widthFix"></image></view></view><w-picker class="vue-ref" vue-id="1" mode="region" data-ref="region" data-event-opts="{{[['^confirm',[['bindCityChange']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></view><view class="form-item"><text class="itemtitle">详细住址</text><input class="input picker" type="text" value=""/></view><view class="form-item"><text class="itemtitle">联系方式</text><input class="input picker" maxlength="11" type="number" value=""/></view><view class="form-item"><text class="itemtitle">验证码</text><text data-event-opts="{{[['tap',[['getcode',['$event']]]]]}}" class="get_code" bindtap="__e">发送验证码</text></view><view class="lijishengji"><button data-event-opts="{{[['tap',[['save',['$event']]]]]}}" class="sengjibtn" bindtap="__e">确定</button></view></view></form></view></view></view>