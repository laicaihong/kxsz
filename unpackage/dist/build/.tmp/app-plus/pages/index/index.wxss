/*签到*/
.dakaciclecont_daka {
	position: fixed;
	bottom: 120rpx;
	right: 10rpx;
	width: 144rpx;
	height: 144rpx;
}
.dakaciclecont_daka_flex {
	-webkit-animation-name: scaleDraw;
	-webkit-animation-timing-function: ease-in-out;
	-webkit-animation-iteration-count: infinite;
	/*动画播放的次数*/
	-webkit-animation-duration: 5s;
	/*动画所花费的时间*/
	position: relative;
	background: url(data:image/png;base64,);
	background-size: 100% 100%;
	width: 144rpx;
	height: 144rpx;
	text-align: center;
}
.dakaciclecont_daka_flex text{
	color: #fff;
	font-size:28rpx;
	line-height: 144rpx;
}
@-webkit-keyframes scaleDraw {

	/*定义关键帧、scaleDrew是需要绑定到选择器的关键帧名称*/
0% {
		-webkit-transform: scale(1);
		        transform: scale(1);
		/*开始为原始大小*/
}
25% {
		-webkit-transform: scale(1.1);
		        transform: scale(1.1);
		/*放大1.1倍*/
}
50% {
		-webkit-transform: scale(1);
		        transform: scale(1);
}
75% {
		-webkit-transform: scale(1.1);
		        transform: scale(1.1);
}
}
@keyframes scaleDraw {

	/*定义关键帧、scaleDrew是需要绑定到选择器的关键帧名称*/
0% {
		-webkit-transform: scale(1);
		        transform: scale(1);
		/*开始为原始大小*/
}
25% {
		-webkit-transform: scale(1.1);
		        transform: scale(1.1);
		/*放大1.1倍*/
}
50% {
		-webkit-transform: scale(1);
		        transform: scale(1);
}
75% {
		-webkit-transform: scale(1.1);
		        transform: scale(1.1);
}
}

